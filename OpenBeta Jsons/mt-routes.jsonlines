{"route_name": "Willbilly", "grade": {"YDS": "5.10c/d", "French": "6b+", "Ewbanks": "21", "UIAA": "VII+", "ZA": "21", "British": "E3 5b"}, "safety": "", "type": {"sport": true}, "fa": "Cowan and Carney May 2015", "description": ["Crimpy face moves with some intermittent cracks, to a large ledge, to another short face of thin face climbing."], "location": ["Starts right where the large belay ledge meets the hillside."], "protection": ["6 bolts to chains. Chains shared with Jameson AND Darling Nikki."], "metadata": {"left_right_seq": "0", "parent_lnglat": [-111.5616, 46.63471], "parent_sector": "Nude Beach SW Face", "mp_route_id": "113018436", "mp_sector_id": "112889937", "mp_path": "Southwest Region|Big Belt Mountains|Avalanche Gulch|Nude Beach SW Face"}}
{"route_name": "Warbird", "grade": {"YDS": "V4", "Font": "6B"}, "safety": "", "type": {"boulder": true}, "fa": "P. Haun", "description": ["Start sitting on the edge of the cool arete feature.  Climb up with crimps and slaps on the arete.  Good rock."], "location": ["Only problem on boulder."], "protection": ["Pads."], "metadata": {"left_right_seq": "0", "parent_lnglat": [-111.16462, 45.26226], "parent_sector": "Warbird Boulder", "mp_route_id": "113530263", "mp_sector_id": "113530257", "mp_path": "Southwest Region|Big Sky Area|Hidden Lakes|District Two (The Meadow)|Warbird Boulder"}}
