{"route_name": "Grease Monkey", "grade": {"YDS": "5.12a", "French": "7a+", "Ewbanks": "25", "UIAA": "VIII+", "ZA": "25", "British": "E5 6a"}, "safety": "", "type": {"sport": true}, "fa": "unknown", "description": ["Thoughtful cruxes with mixture of power and technique. Fun route!"], "location": ["Route to the left of the 10 on the main wall of the crag (east face?)."], "protection": ["Bolts."], "metadata": {"left_right_seq": "2", "parent_lnglat": [-114.5682, 47.0053], "parent_sector": "Alberton Rest Stop", "mp_route_id": "117765388", "mp_sector_id": "106300658", "mp_path": "Northwest Region|Alberton Rest Stop"}}
